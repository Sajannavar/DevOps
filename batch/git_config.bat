@echo off

:: Path to your local Git repository
set REPO_PATH=C:\path\to\your\local\repo

:: Branch name
set BRANCH=main

:: Git username and email (if using HTTPS)
set GIT_USERNAME=your_username
set GIT_EMAIL=your_email@example.com

:: Path to SSH private key (if using SSH)
set SSH_KEY_PATH=%USERPROFILE%\.ssh\id_rsa
